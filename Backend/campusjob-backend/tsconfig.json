{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "typeRoots": ["./node_modules/@types"],
    "types": ["node", "express"] // Asegúrate de incluir "express" aquí
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}